<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.penseapp.acaocontabilidade">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".login.view.activities.LoginActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity android:name=".chat.view.ChatsActivity" />
        <activity android:name=".chat.view.ContactsActivity" />
        <activity android:name=".chat.view.MessagesActivity">
            <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.penseapp.acaocontabilidade.chat.view.TabbedMainActivity" />
        </activity>
        <activity
            android:name=".chat.view.TabbedMainActivity"
            android:label="@string/title_activity_tabbed_main"
            android:theme="@style/AppTheme">
        </activity>
    </application>

</manifest>